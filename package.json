{
  "name": "xatixatixs-coding-game",
  "displayName": "XATIXATIX's Coding Game",
  "description": "In this coding game you can learn the basics about coding with fun, but challenging tasks.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xatixatixs-coding-game.startCodingGame"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "permissions": [
      "webview"
    ],
    "commands": [
      {
        "command": "xatixatixs-coding-game.startCodingGame",
        "title": "Start Coding Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "fs": "0.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
